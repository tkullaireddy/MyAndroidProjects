<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> </shape>
    </item>
    <item android:left="0dp" android:top="0dp"
        android:right="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="#FF000000" /> </shape>
    </item>
</layer-list>